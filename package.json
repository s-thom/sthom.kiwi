{
  "name": "sthom.kiwi",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/inter": "^4.5.7",
    "@fontsource/jetbrains-mono": "^4.5.5",
    "@fontsource/jost": "^4.5.6",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "chroma-js": "^2.4.2",
    "clsx": "^1.1.1",
    "deepmerge": "^4.2.2",
    "lit": "^2.2.1",
    "next": "^12.1.4",
    "next-seo": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "static-path": "^0.0.4",
    "styled-components": "^5.3.5",
    "three": "^0.139.2"
  },
  "scripts": {
    "dev": "concurrently -n next,web-components \"npm run dev:next\" \"npm run dev:web-components\"",
    "dev:next": "next dev",
    "dev:web-components": "esbuild web-components/index.ts --target=es6 --bundle --sourcemap --watch --outfile=public/web-components/index.js",
    "build": "npm run build:web-components && npm run build:next",
    "build:next": "next build",
    "build:web-components": "esbuild web-components/index.ts --target=es6 --bundle --minify --outfile=public/web-components/index.js",
    "start": "next start",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint \"{src,pages}/**/*.{js,jsx,ts,tsx}\"",
    "format": "eslint \"{src,pages}/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.4",
    "@types/chroma-js": "^2.1.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.24",
    "@types/three": "^0.139.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.29",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.2.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.6.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.6.3"
  }
}
